class Solution:
    def maxArea(self, height: List[int]) -> int:
        i=0
        j=len(height)-1
        max_overall= (min(height[i],height[j])*(j-i))
        while(i!=j):
            if height[i]<height[j]:
                i+=1
            else:
                j-=1
            curr_max=(min(height[i],height[j])*(j-i))
            if curr_max>max_overall:
                max_overall=curr_max
        return max_overall
