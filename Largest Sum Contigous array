# from sys import maxint

def fun(arr):
    max_ending_here = 0
    max_so_far = 0
    
    for i in arr:
        max_ending_here += i
        if max_so_far < max_ending_here:
            max_so_far = max_ending_here
        if max_ending_here<0:
            max_ending_here=0
    return max_so_far    
            
arr = [-2, -3, 4, -1, -2, 1, 5, -3]
print(fun(arr))
