Brute Force Method: ---O(n2)
class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        max_count=0
        for i in range(len(s)):
            arr=[]
            count=0
            for j in range(i,len(s)):
                if s[j] in arr:
                    break
                else:
                    arr.append(s[j])
                    count+=1
            if count>max_count:
                max_count=count
        return max_count
    ------------------------------------------------------------------------------------------
    Optimal Soln: ---O(n)
    class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        dic={}
        res=0
        i=0
        for j in range(len(s)):
            if s[j] in dic:
                i= max(dic[s[j]],i)
            
            res=max(res, j-i+1)
            dic[s[j]]=j+1
        return res
